{
  "name": "node-red-contrib-german-holidays",
  "version": "0.0.3",
  "description": "NodeRED nodes to get german holidays",
  "main": "german-holidays.js",
  "scripts": {
    "test": "camo-purge; xo",
    "lintfix": "xo --fix",
    "predev": "npm i -g",
    "dev": "node-red -v"
  },
  "author": {
    "name": "Hypnos3",
    "email": "hypnos3@online.de",
    "url": "https://github.com/hypnos3"
  },
  "contributors": [
    {
      "name": "Hypnos3",
      "url": "https://github.com/hypnos3"
    },
    {
      "name": "sfakir",
      "url": "https://github.com/sfakir"
    }
  ],
  "keywords": [
    "node-red",
    "german-holidays",
    "holidays",
    "german",
    "shutter-control"
  ],
  "homepage": "https://github.com/Hypnos3/node-red-contrib-german-holidays",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hypnos3/node-red-contrib-german-holidays"
  },
  "bugs": {
    "url": "https://github.com/Hypnos3/node-red-contrib-german-holidays/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "camo-purge": "^1.0.2",
    "eslint-polugin-html": "^4.0.3",
    "xo": "^0.20.3"
  },
  "node-red": {
    "nodes": {
      "german-holidays": "german-holidays.js"
    }
  },
  "xo": {
    "space": 4,
    "plugin": "html",
    "global": [
      "$",
      "RED"
    ],
    "extensions": [
      "js",
      "html"
    ],
    "rules": {
      "capitalized-comments": "off"
    }
  }
}